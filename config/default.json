{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "jsonPath": "/api.json",
    "uiPath": "/api"
  },
  "logger": {
    "level": "info"
  },
  "server": {
    "port": "8080"
  },
  "watcher": {
    "watchDirectory": "/watch/**/*.shp",
    "watchOptions": {
      "depth": 2,
      "awaitWriteFinish": true,
      "persistent": true,
      "useFsEvents": false,
      "usePolling": true
    }
  },
  "overseer": {
    "url": "http://127.0.0.1:8081"
  },
  "agentDB": {
    "url": "http://127.0.0.1:8082"
  },
  "externalReadinessUrl": ""
}
