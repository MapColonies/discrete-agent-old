{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "jsonPath": "/api.json",
    "uiPath": "/api"
  },
  "logger": {
    "level": "info"
  },
  "server": {
    "port": "8080"
  },
  "mountDir": "/layerSources",
  "watcher": {
    "watchDirectory": "watch",
    "rootDirNestingLevel": 1,
    "watchOptions": {
      "minTriggerDepth": 1,
      "maxWatchDepth": 2,
      "interval": 1000
    },
    "shpRetry": {
      "retries": 2,
      "factor": 2,
      "minTimeout": 1000,
      "maxTimeout": 60000,
      "randomize": false
    }
  },
  "overseer": {
    "url": "http://127.0.0.1:8081"
  },
  "agentDB": {
    "url": "http://127.0.0.1:8082"
  },
  "externalReadinessUrl": "",
  "httpRetry": {
    "attempts": 5,
    "delay": "exponential",
    "shouldResetTimeout": true
  },
  "S3": {
    "accessKeyId": "minioadmin",
    "secretAccessKey": "minioadmin",
    "endpoint": "localhost:9000",
    "bucket": "mapping-images",
    "forcePathStyle": true,
    "sslEnabled": false
  },
  "classification": {
    "optionsFileLocation": "config/classification.json",
    "storageProvider": "FS"
  }
}
